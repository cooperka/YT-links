{
	"name": "YT Links",
	"description": "Displays quick, searchable links to great YouTube videos. Mostly for personal use.",
	"manifest_version": 2,
	"version": "1.3.1",
	"permissions": [ "tabs" ],
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"16": "images/icon16.png",
			"48": "images/icon48.png",
			"128": "images/icon128.png"
		},
		"default_title": "YT Links",
		"default_popup": "popup.html"
	}
}
